{
  "name": "@sentry/browser",
  "version": "0.1.0",
  "description": "Offical Sentry SDK for browsers",
  "main": "dist/browser.js",
  "typings": "dist/browser.d.ts",
  "typescript": {
    "definition": "dist/browser.d.ts"
  },
  "dependencies": {
    "@sentry/core": "file:../core",
    "raven-js": "^3.18.1"
  },
  "devDependencies": {
    "@types/jest": "^21.1.6",
    "@types/node": "^8.0.51",
    "@types/puppeteer": "^0.12.4",
    "jest": "^21.2.1",
    "prettier": "^1.8.2",
    "puppeteer": "^0.13.0",
    "ts-jest": "^21.2.2",
    "ts-loader": "^3.1.1",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.6.0",
    "tslint-eslint-rules": "^4.1.1",
    "typescript": "^2.6.1",
    "vrsource-tslint-rules": "^5.8.0",
    "webpack": "^3.8.1"
  },
  "engines": {
    "node": ">=6.9.5 <9.0.0",
    "npm": ">=3.10.7 <6.0.0",
    "yarn": ">=1.0.2 <2.0.0"
  },
  "scripts": {
    "build-browser": "webpack --config webpack.config.js",
    "clean": "rm -rf ./dist",
    "dist": "npm run clean && tsc -p tsconfig.json && npm run build-browser",
    "test": "npm run dist && jest --forceExit",
    "test:watch": "npm run dist && jest --watch --notify"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/__mocks__"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "\\.d\\.(jsx?|tsx?)$",
      "\\.no-jest\\.(jsx?|tsx?)$",
      "/dist/"
    ]
  },
  "author": "Sentry",
  "license": "MIT"
}
