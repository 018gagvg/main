{
  "name": "@sentry/platform-browser",
  "version": "0.1.0",
  "description": "Offical Sentry SDK for browsers",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "dependencies": {
    "raven-js": "^3.18.1"
  },
  "devDependencies": {
    "@types/jest": "^21.1.2",
    "@types/node": "^8.0.32",
    "jest": "^21.2.1",
    "prettier": "^1.7.4",
    "ts-jest": "^21.1.2",
    "ts-loader": "^2.3.7",
    "ts-node": "^3.3.0",
    "typescript": "^2.5.3",
    "webpack": "^3.6.0"
  },
  "engines": {
    "node": ">=6.9.5 <9.0.0",
    "npm": ">=3.10.7 <6.0.0",
    "yarn": ">=1.0.2 <2.0.0"
  },
  "scripts": {
    "preinstall": "node -e \"if(process.env.npm_execpath.indexOf('yarn') === -1) throw new Error('Please use Yarn instead of NPM to install dependencies. See: https://yarnpkg.com/lang/en/docs/install/')\"",
    "clean": "rm -rf ./dist",
    "dist": "npm run clean && tsc -p tsconfig.json",
    "test": "jest",
    "test:watch": "jest --watch --notify"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "author": "Sentry",
  "license": "MIT"
}
