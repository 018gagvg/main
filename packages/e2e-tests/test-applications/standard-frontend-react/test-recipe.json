{
  "$schema": "../../test-recipe-schema.json",
  "testApplicationName": "standard-frontend-react",
  "buildCommand": "yarn install --pure-lockfile && npx playwright install && yarn build",
  "tests": [
    {
      "testName": "Playwright tests",
      "testCommand": "yarn test"
    }
  ],
  "versions": [
    {
      "dependencyOverrides": {
        "react": "^18",
        "react-dom": "^18"
      }
    },
    {
      "dependencyOverrides": {
        "react": "^17",
        "react-dom": "^17"
      }
    },
    {
      "dependencyOverrides": {
        "react": "^16",
        "react-dom": "^16"
      }
    }
  ],
  "canaryVersions": [
    {
      "dependencyOverrides": {
        "react": "latest",
        "react-dom": "latest"
      }
    }
  ]
}
