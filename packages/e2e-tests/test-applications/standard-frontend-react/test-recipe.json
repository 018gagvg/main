{
  "$schema": "../../test-recipe-schema.json",
  "testApplicationName": "standard-frontend-react",
  "buildCommand": "pnpm install && npx playwright install && pnpm build",
  "tests": [
    {
      "testName": "Playwright tests",
      "testCommand": "pnpm test"
    }
  ],
  "versions": [
    {
      "dependencyOverrides": {
        "typescript": "3.8.3"
      }
    }
  ],
  "canaryVersions": [
    {
      "dependencyOverrides": {
        "react": "latest || *",
        "react-dom": "latest || *"
      }
    }
  ]
}
